<!-- Modify it at your will. -->

<group name="local,syslog,">

  <rule id="200000" level="1">
     <if_sid>1002</if_sid>
     <match>comm="iptables" path="/var/log/audit/audit.log"</match>
     <description>Unknown iptables SELinux violation</description>
  </rule>


</group> <!-- SYSLOG,LOCAL -->


